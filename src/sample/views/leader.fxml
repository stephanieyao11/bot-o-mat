<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>


<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.ControllerLeader">
<children>
    <AnchorPane prefHeight="480.0" prefWidth="850.0">
        <ImageView fitHeight="480.0" fitWidth="850.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../media/leaderboard.png" />
            </image>
        </ImageView>
        <Label layoutX="200.0" layoutY="60.0" prefHeight="48.0" prefWidth="250.0" text="LEADERBOARD" textFill="WHITE">
            <font>
                <Font size="32.0" />
            </font>
        </Label>
        <Label fx:id="robot1Leader" layoutX="100.0" layoutY="120.0" text="Robot 1 Score: " textFill="WHITE">
            <font>
                <Font size="22.0" />
            </font>
        </Label>
        <Label fx:id="robot2Leader" layoutX="100.0" layoutY="156.0" text="Robot 2 Score: " textFill="WHITE">
            <font>
                <Font size="22.0" />
            </font>
        </Label>



    </AnchorPane>

</children>
</VBox>