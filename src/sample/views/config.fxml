<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.collections.*?>
<?import java.lang.*?>

<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.ControllerConfig">
    <children>
        <AnchorPane prefHeight="408.0" prefWidth="615.0">
            <children>
                <ImageView fitHeight="430.0" fitWidth="780.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../media/robotBackground.png" />
                    </image>
                </ImageView>
                <Label layoutX="184.0" layoutY="33.0" text="Configurations" textFill="WHITE">
                    <font>
                        <Font size="38.0" />
                    </font>
                </Label>
                <Label layoutX="51.0" layoutY="100.0" text="Name" textFill="WHITE">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Label>
                <TextField fx:id="name" layoutX="50.0" layoutY="150.0" prefHeight="23.0" prefWidth="114.0" promptText="Enter Name" />
                <ComboBox fx:id="robotCombo" layoutX="300.0" layoutY="150.0" prefWidth="90.0" promptText="Choose Robot Type">
                    <items>
                        <FXCollections fx:factory="observableArrayList">
                            <String fx:value="Unipedal" />
                            <String fx:value="Bipedal" />
                            <String fx:value="Quadrupedal" />
                            <String fx:value="Arachnid" />
                            <String fx:value="Radial" />
                            <String fx:value="Aeronautical" />
                        </FXCollections>
                    </items>
                </ComboBox>


                <TextField fx:id="name1" layoutX="50.0" layoutY="200.0" prefHeight="23.0" prefWidth="114.0" promptText="Enter Name" />
                <ComboBox fx:id="robotCombo1" layoutX="300.0" layoutY="200.0" prefWidth="90.0" promptText="Choose Robot Type">
                    <items>
                        <FXCollections fx:factory="observableArrayList">
                            <String fx:value="Unipedal" />
                            <String fx:value="Bipedal" />
                            <String fx:value="Quadrupedal" />
                            <String fx:value="Arachnid" />
                            <String fx:value="Radial" />
                            <String fx:value="Aeronautical" />
                        </FXCollections>
                    </items>
                </ComboBox>



                <Button fx:id="next" layoutX="279.0" layoutY="342.0" mnemonicParsing="false" onAction="#setNext" text="Next" />
            </children>
        </AnchorPane>
    </children>
</VBox>
